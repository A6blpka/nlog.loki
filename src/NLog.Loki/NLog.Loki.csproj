    <Company />
    <Product />
    <Copyright></Copyright>
    <PackageProjectUrl>https://github.com/corentinaltepe/nlog.loki</PackageProjectUrl>
    <RepositoryUrl>https://github.com/corentinaltepe/nlog.loki</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
